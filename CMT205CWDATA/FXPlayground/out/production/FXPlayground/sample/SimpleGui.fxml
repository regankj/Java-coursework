<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?language JavaScript?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="okBtn" onAction="printOutput();" layoutX="56.0" layoutY="104.0" mnemonicParsing="false" text="OK" />
      <TextField fx:id="inputTxt" layoutX="56.0" layoutY="51.0" />
      <TextArea fx:id="outputTxt" layoutX="56.0" layoutY="155.0" prefHeight="200.0" prefWidth="488.0" />
        <fx:script>
            function printOutput()
            {
                outputTxt.setText(inputTxt.getText());
            }
        </fx:script>
      </children>
</AnchorPane>
